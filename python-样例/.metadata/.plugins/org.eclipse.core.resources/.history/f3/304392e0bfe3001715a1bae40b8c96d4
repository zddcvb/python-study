import os


basePath="F:/部门对接/编辑所需文件20170930"
def readFile(path):
    if !os.path.exists(path):
        return ;
    dirs = os.listdir(path)
    for index in dirs:
        newPath=basePath+"/"+index
        if os.path.isdir(newPath):
            readFile(newPath)
        else :
            print index;
readFile(basePath);
        
      #     print index;
# print listdir